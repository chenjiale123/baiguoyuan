.hidden{visibility: hidden;}
input{line-height: 56rpx;height:56rpx;width:294rpx;font-size:28rpx;}
.container{padding:100rpx 0;background-size: cover;width: 100%;height: 100%;}
.container .icon-logo{width:180rpx;height:90rpx;}
.form-item-box{padding:90rpx 0;font-size:28rpx;}
.form-item{display: flex;border-bottom:1rpx solid rgb(229, 229, 229);padding:12rpx 0;}
.form-item label{display: flex;flex-direction: column;justify-content: center;margin-right: 28rpx;}
.mt40{margin-top:40rpx;}
.form-item .icon-iphone{width: 44rpx;height:44rpx;}
.form-item .icon-lock{width:44rpx;height:44rpx;}
.icon-close{width:30rpx;height:30rpx;}
.form-item .btn-vcode{text-align:center;font-size:28rpx;color:#39d963;border:1rpx solid #39d963;line-height:56rpx;border-radius:30rpx;width:188rpx;padding:0;}
.form-item .vcode-off{border:1rpx solid rgb(178,178,178);color: #999999;}
.form-des{font-size:26rpx;color:rgb(163,163,163)}
.form-des text{color:rgb(87,107,149);}
.mt16{margin-top:16rpx;}
.form .btn-login{text-align:center;font-size: 36rpx;line-height: 45px;border:1rpx solid #39d963;background:#39d963;color:#FFF;border-radius: 47px;}
.form .btn-off{background: #999999;border:1rpx solid #999999;}