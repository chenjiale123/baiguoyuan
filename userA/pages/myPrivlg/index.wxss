page{min-height:100%;}
.container{padding:24rpx 30rpx;height:auto;min-height:100%;}
.hidden{display:none!important;}
.h-none{visibility: hidden!important}
.c-t{width:742rpx;height: 360rpx;background-size: 100% 100%;color: #FFF;display: flex;flex-direction: column;justify-content: space-between;}
.t-up{padding:16rpx 6%;display: flex;justify-content: space-between;}
.f-h1{font-size: 48rpx;font-weight: 600;margin-top:17rpx;}
.expires{font-size: 24rpx;line-height: 36rpx;}
.u-box{display: flex;margin-top:-30rpx;margin-bottom:66rpx;margin-left:8%;}
.jifen{margin-right:10rpx;font-size:22rpx;font-weight: 600;width: 120rpx;height: 34rpx;text-align: center;line-height: 34rpx;border-radius:50rpx;border:1rpx solid #FFF;}
.up-r{font-size: 24rpx;text-decoration: underline;margin-top: 10rpx;}

/* .t-down{position: relative;} */
.line-box{position: relative;padding:3rpx;background:rgb(138,215,69);border-radius:26rpx;margin-left: 8%;margin-top: 60rpx;width:84%;display: flex;justify-content: center;align-items: center;}
.line-silver{background: rgb(199,204,212)}
.line-gold{background: rgb(228, 198, 143)}
.line-dem{margin-top:71rpx;background: rgb(126,126,126)}
.line{background-size:cover;height: 20rpx;width:100%;border-radius:20rpx;}
.baseline{position:absolute;right:0;top:3rpx;width:100%;height:20rpx;line-height:20rpx;border-radius:20rpx;text-align:right;font-size:18rpx;color:#FFF;font-weight:bold;background:rgb(162,223,107);}
.bg-silver{background: rgb(211, 215, 221)}
.bg-gold{background: rgb(234, 210, 166)}
.bg-dem{background: rgb(175,175,175)}
/* .bl-t{margin-right: 10rpx} */
.bl-t{position:absolute;right:10rpx}
.br-t{position: absolute;left: 10rpx;font-size: 18rpx;line-height: 20rpx;}
.icon-right{margin-left:10rpx;width: 8rpx;height: 12rpx;}
.r-u{position:absolute;transform:translate(50%,-115%);width:88rpx;height: 38rpx;background-size: cover;font-size:22rpx;font-weight:bold;color: rgb(126,211,33);text-align: center;line-height: 38rpx;display: flex;justify-content: center;align-items: center;}
.r-trans{transform:translate(20%,-115%);}
.l-trans{transform:translate(80%,-115%)}
.j-dot{position:absolute;right:100%;top:-1.5rpx;width:28rpx;height:29rpx;transform:translateX(50%)}
.silver-txt{font-size: 20rpx;margin-left: 8%;margin-top: 5rpx;}
.m-c{color: rgb(102,102,102)}
.g-c{color: rgb(194,129,6)}
.d-c{color: rgb(51,51,51)}


.c-b{width: 100%;flex:1;display: flex;flex-direction: column;}
.b-t{display: flex;flex-direction: row;justify-content: center;align-items: center;width: 100%;}
.b-t-dot{width:8rpx;height: 8rpx;border-radius:50%;background: rgb(195,151,94);margin:0 10rpx;}
.b-t-line{width:174rpx;height:2rpx;border-bottom:1rpx dashed rgb(195,151,94)}
.f-h2{font-size: 24rpx;color: rgb(195,151,94);margin:0 10rpx;}

.nav-box{display: flex;justify-content: center;margin-top: 36rpx;}
.item-nav-box{flex:1;}
.item-nav{display: flex;justify-content: center;align-items: center;height: 38px;}
.item-nav .icon-lev{width: 56rpx;height: 56rpx;}
.selected .icon-lev{width: 76rpx;height: 76rpx;}
.item-nav .line{flex:1;height: 4rpx;background: rgb(245,245,245)}
.selected .line{background: rgb(248,231,28)}
 .on .line{background: rgb(248,231,28)}
 .active-on .icon-lev{width:76rpx;height: 76rpx;}
 .active-off .icon-lev{width: 56rpx;height: 56rpx;} 
.w-line{flex:1;height: 4rpx;background: rgb(255,255,255)}
.lev-name{text-align: center;font-size: 24rpx;color: rgb(102,102,102);margin-top: 8rpx;}



.b-pg-box{flex:1;position:relative;margin-top: 30rpx;padding:20rpx 0;border: 1rpx solid #F2F2F2;
border-radius:20rpx;box-shadow: 0 0 30rpx rgb(244,244,244);}
