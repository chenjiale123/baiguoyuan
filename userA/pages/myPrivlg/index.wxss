/* .b-silver .b-item-box,.b-gold .b-item-box,.b-demon .b-item-box{flex: 1;} */
.b-item-box{flex: 1;}

.arrow-up{position:absolute;top:-28rpx;width:64rpx; height:29rpx;}
.b-normal .arrow-up{left: 8%}
.b-silver .arrow-up{left: 33%;}
.b-gold .arrow-up{left: 58%;}
.b-demon .arrow-up{left: 83%}
.b-item-box{display: flex;justify-content: center;align-items: center;width: 100%;}
.b-item{flex:1;text-align: center;}
.item-image{display: inline-block;position: relative;}
.icon-corn{position: absolute;top:-2rpx;right:-20rpx;width: 53rpx;border:4rpx solid rgb(237,210,160);
  background: rgb(224,190,125);
  border-radius: 26rpx;font-size: 20rpx;color: #FFF}
