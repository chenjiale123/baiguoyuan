@import "/pages/template/index";
.container{display: flex;flex-direction: column;height: auto;min-height: 100%;}
.swiper-box,swiper,swiper-item{width:100%;height:750rpx;display: flex;flex-direction:column;justify-content: center;align-items: center;}
.slide-image{width: 100%;}
.c-m-box{width:100%;background: rgb(245,245,245);padding-bottom: 20rpx;}
.m-t-box{display: flex;justify-content: space-between;padding: 6rpx 24rpx;background: #FFF;}
.m-t{padding: 4rpx 0;font-size: 30rpx;line-height: 42rpx;font-weight: 600;color: rgb(51,51,51);display:flex;align-items:center;align-content: center;flex:1;text-indent:10rpx;}
.icon-share{width: 30rpx;height: 30rpx;}
.share-box{margin-left: 12rpx;width: 65rpx;text-align: center;margin-top: 8rpx;background-color:#FFF;
  padding:0;line-height:1.5;}
button::after {
  border: 0;
}
.share-t{font-size: 22rpx;color: rgb(102,102,102);}

.m-des{font-size: 24rpx;color: rgb(51,51,51);padding: 4rpx 20rpx 4rpx 24rpx;background: #FFF;}

.o-box{background: #FFF;margin-top: 20rpx;padding: 0 24rpx;}
.o-t-w{display: flex;justify-content: space-between;font-size: 28rpx;height: 80rpx;line-height: 80rpx;}
.t-r{display:flex;justify-content:center;align-items:center;color: rgb(153,153,153);font-size: 24rpx;}
.icon-right{width: 12rpx;height: 22rpx;margin-left: 10rpx;}

.m-p{font-size:24rpx;color: rgb(255,160,5);
margin-left:10rpx;display:flex;flex-direction:column;justify-content:flex-end;}
.p-wrap{position:relative;display: flex;padding:0rpx 24rpx;justify-content: space-between;background-size: cover;height:  96rpx;}
.p-box{color: rgb(255,255,255);display: flex;line-height: 96rpx; align-items: center;}
.price{display: flex;align-items: baseline;}
.icon_pintuanjia{width: 24rpx;height: 60rpx;margin-right: 12rpx;}
.yen{margin-right: 5rpx;}
.yen,.p-small{font-size: 40rpx;display: flex;flex-direction: column;justify-content:flex-end;margin-bottom: 2px;}
.p-big{font-size: 48rpx;}

.del-p-box{display: flex;flex-direction: column;color: #fff;margin-left: 20rpx;justify-content:center;}
.del-p1{font-size:22rpx;}
.del-p2{font-size: 24rpx;text-decoration:line-through;transform:translateY(6rpx);
}
.corner-mark{width:82rpx;height: 28rpx;font-size: 20rpx;color: #fff;text-align: center;line-height: 26rpx;border: 1rpx solid #fff;border-radius: 30rpx;margin-right: 20rpx;}
.right-box{position:absolute;right:0;top:0;width: 312rpx;height:100%;background-size: cover;display:flex;flex-direction:column;justify-content: center;}
.t-t{font-size: 22rpx;text-align: center;margin-bottom: 8rpx;color: #666}
.time-box{display: flex;justify-content: center;font-size: 18rpx;align-items: center;color: rgb(153,153,153);padding-left: 10rpx;}
.t-bg{color:#000;font-size: 24rpx;width: 36rpx;height: 36rpx;border-radius: 10rpx;background: #fff;text-align: center;line-height: 36rpx;margin:0 3rpx;}

.notice{border:1rpx dashed rgb(220,220,220);height: 110rpx;text-align:center;
background: rgba(248,138,11,0.05);font-size: 40rpx;font-weight: 600;color: rgb(248,138,11)}
.n-b{margin-top: 10rpx;}
.n-s{font-size: 24rpx;}

.c-img-box{margin-top: 10rpx;padding: 0 24rpx 110rpx;}
.c-img-box .pic-w{display: flex;flex-direction: column;}
.c-img-box,.icon-detailImg{width:100%;margin: 0 auto;} 
.b-t{font-size: 22rpx;margin-bottom: 30rpx;line-height: 80rpx;height: 80rpx;border-bottom:1px solid rgb(220,220,220)}

.c-b-box{position:fixed;bottom:0;width:100%;height:90rpx;background: rgb(248,138,11);display: flex;font-size: 34rpx;font-weight: 600;color: #FFF;}
.join-btn{width: 200rpx;background: #FFF;justify-content: center;align-items: center;display:flex;flex-direction:column;border-top:solid 2rpx #f2f2f2}
.icon_home{width: 38rpx;height: 40rpx;margin-bottom:5rpx;}
.index{font-size: 20rpx;color: rgb(102,102,102)}
.off{background: #ccc;}
.open-btn{flex:1;text-align: center;line-height: 96rpx;}

.mask{position: fixed;left: 0;top: 0;width: 100%;height: 100%;background: rgba(0,0,0,.7);display: flex;justify-content: center;}
.club-box{position:fixed;top:50%;left:50%;transform:translate(-50%,-50%);
background: #FFF;width:700rpx;padding-bottom:58rpx;border-radius: 16rpx;}
.c-t-box{position: relative;height: 100rpx;line-height: 100rpx;text-align: center;font-size: 40rpx;color: rgb(51,51,51);font-weight: 600}
.icon-close{position: absolute;right: 29rpx;top: 50%;transform:translateY(-50%);width:30rpx;height: 30rpx;}
.club-list{width:auto;background: rgb(245,245,245);border-top: rgb(236,236,236);}
.club-box .club-list{background-color: #fff;border-top: 1rpx solid rgb(236,236,236);border-bottom: 1rpx solid rgb(236,236,236)}

.item{display: flex;padding:26rpx 0 26rpx 26rpx;border-bottom:1rpx solid rgb(244,244,244);background:#FFF;}
.club-box .item{margin:0 24rpx;padding:26rpx 0;}

.item:last-child{margin-bottom:0}
.item-left{margin-right: 16rpx;}
.item-image{width: 100rpx;height: 100rpx;border-radius: 50%;}
.item-mid{flex:1;display: flex;flex-direction:column;justify-content: space-between;}
.it-m-t{font-size: 28rpx;color: #333;}
.time{font-size: 24rpx;color: rgb(51,51,51)}
.item-right{width: 148rpx;position: relative;}

.cantuan{position: absolute;right: 0rpx;top:50%;transform: translateY(-50%);width: 134rpx;height: 58rpx;line-height: 58rpx;border-radius: 30rpx;font-size: 28rpx;text-align: center;background-color: rgb(255,159,6);box-shadow: rgba(248,138,10,0.46) -1rpx 2rpx 6rpx 0}
.can_t{background: rgb(255,159,6);text-align: center;width:132rpx;height:58rpx;line-height: 58rpx;color: #FFF;font-size: 28rpx;border-radius: 58rpx;}

.cl-p{background: #FFF;padding:0;}
.i-p{border:none;box-shadow:none;border-top:1rpx solid rgb(220,220,220);border-radius:0;margin-bottom: 0}
.i-c{justify-content: center;font-size: 24rpx;color: rgb(51,51,51);font-weight: normal;flex: none;text-align: right}
.i-n{font-weight: normal;font-size: 24rpx;margin-bottom: 8rpx;letter-spacing: 1rpx;}
.m-c{justify-content: center;}
.n-c{color: rgb(255,159,6);margin:0 5rpx;}

.tips-template{position: fixed;background: rgba(0, 0, 0,0.5);z-index: 999;top: 0;height: 100%;width: 100%;}
.tipsAll{position: fixed;display: flex;flex-direction: column;justify-content: space-between;bottom: 0;height: 718rpx;
  background: #fff;width: 100%;z-index:999;}
.tips-btn{font-size: 34rpx;height: 96rpx;line-height: 96rpx;text-align: center;color: rgb(54,209,95);
  border-top: 1px solid rgb(220, 220, 220)}
.tips-line{background: #FFF;justify-content: space-between;padding: 0 24rpx;}

.spec-info{
  display:flex;
  align-items:center;
  background:#fff;
  padding: 5rpx 0;
}
.tipsline{display: flex; align-items: center; background: #fff;border-top: 1rpx solid #f2f2f2;}
.tip-img{width: 12rpx;height: 22rpx;margin:0 25rpx 0 68rpx;}
.iphoneXBt{
  border-bottom: 64rpx #fff solid;
  height: 154rpx;
}